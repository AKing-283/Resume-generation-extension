{
  "name": "awesome-web-app",
  "version": "1.0.1",
  "description": "Made with ❤️ for students/developers by Puspak Dakkata. Instantly generate beautiful developer resumes from your codebase, right inside VS Code.",
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:resume-generator-for-developers.generateResume"
  ],
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "webpack --mode production",
    "test": "jest"
  },
  "keywords": [
    "vscode",
    "extension",
    "resume",
    "developer",
    "generator",
    "students"
  ],
  "author": "Puspak Dakkata <dpreddy294@gmail.com>",
  "displayName": "Developer Resume Generator",
  "categories": ["Other"],
  "homepage": "https://github.com/AKing-283/Resume-generation-extension",
  "license": "MIT",
  "publisher": "PuspakDakkata",
  "engines": {
    "vscode": "^1.70.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "puppeteer": "^24.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simple-git": "^3.28.0"
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Resume Generator Settings",
      "properties": {
        "resumeGenerator.geminiApiKey": {
          "type": "string",
          "default": "",
          "description": "Your Gemini API Key for AI resume generation"
        },
        "resumeGenerator.githubUsername": {
          "type": "string",
          "default": "",
          "description": "Your GitHub username for importing profile data"
        }
      }
    },
    "commands": [
      {
        "command": "resume-generator-for-developers.generateResume",
        "title": "Generate Developer Resume",
        "category": "Resume Generator"
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.47.0",
    "jest": "^29.6.0",
    "nodemon": "^3.0.1",
    "vscode": "^1.1.37",
    "webpack": "^5.88.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AKing-283/Resume-generation-extension"
  }
}
